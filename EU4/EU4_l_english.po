msgid "§g- Increase by §G+$NUM$%§g (level $LEVEL$)§W"
msgstr "§g- §G+$NUM$%§g 증가 ($LEVEL$ 레벨)§W"

msgctxt "CV_RESEARCHED_ADMEFF_TOOLTIP코0"
msgid "+ Increase by §G+$NUM$%§! (level $LEVEL$)"
msgstr "+ §G+$NUM$%§! 증가 ($LEVEL$ 레벨)"

msgctxt "CV_NOTRESEARCHED_ADMEFF_TOOLTIP코0"
msgid "§g- Increase by §G+$NUM$%§g (level $LEVEL$)§W"
msgstr "§g- §G+$NUM$%§g 증가 ($LEVEL$ 레벨)§W"

msgctxt "CV_RESEARCHED_DEVEFF_TOOLTIP코0"
msgid "+ Increase by §G+$NUM$%§! (level $LEVEL$)"
msgstr "+ §G+$NUM$%§! 증가 ($LEVEL$ 레벨)"

msgctxt "CV_NOTRESEARCHED_DEVEFF_TOOLTIP코0"
msgid "§g- Increase by §G+$NUM$%§g (level $LEVEL$)§W"
msgstr "§g- §G+$NUM$%§g 증가 ($LEVEL$ 레벨)§W"

msgctxt "CV_RESEARCHED_ACCP_CULT_TOOLTIP코0"
msgid "+ Increase by §G+$NUM$§! (level $LEVEL$)"
msgstr "+ §G+$NUM$§! 증가 ($LEVEL$ 레벨)"

msgctxt "CV_NOTRESEARCHED_ACCP_CULT_TOOLTIP코0"
msgid "§g- Increase by §G+$NUM$§g (level $LEVEL$)§W"
msgstr "§g- §G+$NUM$§g 증가 ($LEVEL$ 레벨)§W"

msgctxt "CV_RESEARCHED_NAVALMORALE_TOOLTIP코0"
msgid "+ Improve by §G+$NUM$§! (level $LEVEL$)"
msgstr "+ §G+$NUM$§! 증가 ($LEVEL$ 레벨)"

msgctxt "CV_NOTRESEARCHED_NAVALMORALE_TOOLTIP코0"
msgid "§g- Improve by §G+$NUM$§g (level $LEVEL$)§W"
msgstr "§g- §G+$NUM$§g 증가($LEVEL$ 레벨)§W"

