msgctxt "GAIN_CB코0"
msgid "§Y$ACTIVE$§! gain §Y$TYPE$§W Casus Belli on §Y$WHO$§W.\n"
msgstr "§Y$ACTIVE$§!으로 §Y$TYPE$§W 의 전쟁 명분을 §Y$WHO$§W에게서 얻었습니다.\n"

msgctxt "LOSE_CB코0"
msgid "§Y$ACTIVE$§! lose §Y$TYPE$§W Casus Belli on §Y$WHO$§W.\n"
msgstr "§Y$ACTIVE$§!으로 §Y$TYPE$§W 의 전쟁 명분을§Y$WHO$§W에게서 잃었습니다.\n"

msgctxt "REV_GAIN_CB코0"
