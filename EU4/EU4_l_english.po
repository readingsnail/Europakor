msgid "+ Allow §G$NUM$§! States (level $LEVEL$)"
msgstr "+ §G$NUM$§!개의 주 허용 ($LEVEL$ 레벨)"

msgctxt "CV_NOTRESEARCHED_MAXSTATES_TOOLTIP코0"
msgid "§g- Allow §G$NUM$§g States (level $LEVEL$)§W"
msgstr "§g-  §G$NUM$§g개의 주 허용 ($LEVEL$ 레벨)§W"

msgctxt "CV_RESEARCHED_PRODEFF_TOOLTIP코0"
msgid "+ Increase by §G+$NUM$%§! (level $LEVEL$)"
msgstr "+ §G+$NUM$%§! 증가 ($LEVEL$ 레벨)"

