msgstr "종주국에 의해 유용됨"

msgctxt "SEND_OFFICERS코0"
msgid "Send Officers"
msgstr "장교 파견"

msgctxt "NO_EXPENSE_SEND_OFFICERS코0"
msgid "We are not sending officers anywhere."
msgstr "파견한 장교가 없습니다."

msgctxt "send_officers코0"
msgid "Overlord Sends Officers"
msgstr "종주국의 장교 파견"

msgctxt "DIVERT_TRADE코0"
msgid "Divert Trade"
msgstr "교역 독점"

msgctxt "divert_trade코0"
msgid "Overlord Diverts Trade"
msgstr "종주국의 교역 독점"

msgctxt "SEIZE_TERRITORY코0"
