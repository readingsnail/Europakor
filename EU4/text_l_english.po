"$REASONS$"

msgctxt "alert_unconditional_surrender_instant코0"
msgid "§YUnconditional Surrender§!"
msgstr "§Y무조건 항복§!"

msgctxt "alert_unconditional_surrender_delayed코0"
msgid ""
"The following enemies put down their weapons and have surrendered "
"unconditionally:\n"
"$LIST$.\n"
"\n"
