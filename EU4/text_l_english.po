msgid "Buildings"
msgstr "건물"

msgctxt "MISSIONARY_CONSTRUCTIONS코0"
msgid "Missionaries"
msgstr "선교사"

msgctxt "DAYS_LEFT코0"
msgid "days left."
msgstr "일 남음."

msgctxt "LEDGER_NEIGHBOUR코0"
msgid "Neighbors"
msgstr "이웃"

msgctxt "LEDGER_LARGEST코0"
msgid "Largest"
msgstr "제일 큰"

msgctxt "LEDGER_FRIENDS코0"
msgid "Friends"
msgstr "친구들"

msgctxt "LEDGER_ENEMIES코0"
