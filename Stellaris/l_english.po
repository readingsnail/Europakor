msgstr "§H$topbar_button_factions_name$§!"

msgctxt "topbar_button_factions_delayed코0"
msgid ""
"§EOpen the factions view, showing the current factions within your empire.§!"
msgstr "§E당신의 제국에 있는 현재 파벌들을 보여줍니다.§!"

msgctxt "topbar_button_policies_name코0"
msgid "Policies And Edicts"
msgstr "정책과 시행령"

msgctxt "topbar_button_policies_instant코0"
msgid "§H$topbar_button_policies_name$§!"
msgstr "§H$topbar_button_policies_name$§!"

msgctxt "topbar_button_policies_delayed코0"
msgid ""
"§EOpens the Policies view where you can inspect and activate Empire Edicts "
"and Policies.§!"
msgstr "§E제국 시행령과 정책을 점검하고 활성화 가능한 정책 목록을 엽니다.§!"

msgctxt "topbar_button_government_name코0"
msgid "Government"
msgstr "정부"

msgctxt "topbar_button_government_instant코0"
msgid "§H$topbar_button_government_name$§!"
msgstr "§H$topbar_button_government_name$§!"

msgctxt "topbar_button_government_delayed코0"
msgid "§EShows information regarding our budget and government.§!"
msgstr "§E우리의 정부와 예산에 관한 정보를 보여줍니다.§!"

msgctxt "topbar_button_technology_name코0"
msgid "Technology"
msgstr "기술"

msgctxt "topbar_button_technology_instant코0"
msgid "§H$topbar_button_technology_name$§!"
msgstr "§H$topbar_button_technology_name$§!"

msgctxt "topbar_button_technology_delayed코0"
msgid ""
"§EOpens the Technology view where you can research technological "
"advancements.§!"
msgstr "§E당신이 기술적 진보를 연구할 수 있는 기술 목록을 엽니다.§!"

msgctxt "topbar_button_contacts_name코0"
msgid "Contacts"
msgstr "접촉"

msgctxt "topbar_button_contacts_instant코0"
msgid "§H$topbar_button_contacts_name$§!"
msgstr "§H$topbar_button_contacts_name$§!"

msgctxt "topbar_button_contacts_delayed코1"
msgid "§EOpens a list of all known Empires and aliens in the galaxy.§!"
msgstr "§E은하계에 알려진 모든 제국과 종족 목록을 엽니다.§!"

msgctxt "topbar_button_situation_name코0"
msgid "Situation Log"
msgstr "상황 기록"

msgctxt "topbar_button_situation_instant코0"
msgid "§H$topbar_button_situation_name$§!"
msgstr "§H$topbar_button_situation_name$§!"

msgctxt "topbar_button_situation_delayed코0"
msgid "§EOpens a log of all ongoing Events and Special Projects.§!"
msgstr "\"§E모든 진행중인 이벤트와 특별한 계획들의 기록을 엽니다.§!\""

msgctxt "topbar_button_ship_designer_name코0"
msgid "Ship Designer"
msgstr "함선 설계"

msgctxt "topbar_button_ship_designer_instant코0"
