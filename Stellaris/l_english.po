msgstr "§H$topbar_button_species_name$§!"

msgctxt "topbar_button_species_delayed코0"
msgid "§EShows a list of known species.§!"
msgstr "§E알려진 종족 목록을 봅니다.§!"

msgctxt "topbar_button_leaders_name코0"
msgid "Leaders"
msgstr "지도자들"

msgctxt "topbar_button_leaders_instant코0"
