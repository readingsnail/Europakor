msgid "$IMPERIALIST_SUPREMACY_TRADITION$"
msgstr "$IMPERIALIST_SUPREMACY_TRADITION$"

msgctxt "IMPERIALIST_SUPREMACY_TRADITION_DESC코0"
msgid ""
"Adopting any Traditions emphasizing §YSupremacy§! will please the {Root."
"GetName}."
msgstr "§Y우월§!을 중시하는 전통을 채택하면 {Root.GetName}을(를) 기쁘게 할 것입니다."

msgctxt "IMPERIALIST_SLAYER코0"
