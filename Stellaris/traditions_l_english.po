"$tr_synchronicity_machine_adopt_effect$\n"
"\n"
"$tr_synchronicity_finish_effect$"

msgctxt "tr_synchronicity_machine_adopt_effect코0"
msgid ""
"§E$TRADITIONS_ADOPT$:§!\n"
"§Y$MOD_POP_ROBOT_BUILD_SPEED_MULT$§! increased by §G+33%§!."
msgstr ""
"§E$TRADITIONS_ADOPT$:§!\n"
"§Y$MOD_POP_ROBOT_BUILD_SPEED_MULT$§!가 §G+33%§! 증가합니다."

msgctxt "tr_synchronicity_self_preservation코0"
msgid "Self-Preservation Protocols"
msgstr "자가-보존 규약"

