msgid "$tr_synchronicity_hive_adopt$"
msgstr "$tr_synchronicity_hive_adopt$"

msgctxt "tr_synchronicity_machine_adopt_desc코0"
msgid ""
"$tradition_synchronicity_desc$\n"
"\n"
"$tr_synchronicity_machine_adopt_effect$\n"
"\n"
